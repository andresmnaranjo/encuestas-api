{
  "name": "apipersonal",
  "version": "0.0.0",
  "description": "Api personal",
  "main": "src/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=production nodemon src/index.js",
    "start": "node src/index.js",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout=120000 --reporter spec --exit",
    "generate-secret": "node -e console.log(require('crypto').randomBytes(256).toString('base64'));",
    "lint": "eslint . && prettier --check \"**/*.{json,md,html,yaml,css}\"",
    "lint:fix": "eslint . --fix && prettier --write \"**/*.{json,md,html,yaml,css}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,md,html,yaml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@kazaar/express-error-handler": "^2.5.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "celebrate": "^12.1.1",
    "convict": "^5.2.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "express-promise-router": "^3.0.3",
    "express-validator": "^6.3.0",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.21",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.10",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "mocha": "^8.0.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=10.16"
  }
}
